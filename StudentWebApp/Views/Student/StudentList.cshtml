@model List<StudentWebApp.Context.tbl_student>

<table>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Description</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Fname</td>
            <td>@item.Email</td>
            <td>@item.Mobile</td>
            <td>@item.Description</td>
            <td><a href="#">Edit</a></td>
            <td><a href="@Url.Action("Delete", new {item.Id ,@class="btn btn-danger"})">Delete</a></td>
        </tr>
    }
   
